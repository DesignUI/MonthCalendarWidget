   </LinearLayout>
   <LinearLayout
      android:id="@+id/week3ll"
      android:orientation="horizontal" style="@style/LL_Week">
      <TextView android:id="@+id/date31" style="@style/Text_Dates"/>
      <TextView android:id="@+id/date32" style="@style/Text_Dates"/>
      <TextView android:id="@+id/date33" style="@style/Text_Dates"/>
