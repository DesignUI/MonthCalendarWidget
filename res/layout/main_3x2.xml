<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
   android:id="@+id/mainll"
   xmlns:android="http://schemas.android.com/apk/res/android"
   android:orientation="vertical"
   android:layout_width="@dimen/widget_width_3x2"
   android:layout_height="@dimen/widget_height_3x2"
   android:paddingLeft="@dimen/widget_paddingLR_3x2"
   android:paddingRight="@dimen/widget_paddingLR_3x2"
   android:paddingBottom="@dimen/widget_paddingB_3x2"
   android:weightSum="66" android:background="@drawable/widget_bg">
   <RelativeLayout
      android:id="@+id/navrl"
      android:layout_width="match_parent" android:layout_height="0dp"
      android:layout_weight="12">
      <TextView
         android:id="@+id/monthyear"
         style="@style/Text_MonthYear" android:focusable="true"/>
      <ImageButton
         android:id="@+id/prevmonth"
         android:layout_alignParentLeft="true" android:paddingRight="25dp"
         android:src="@drawable/selector_prev" style="@style/Img_Arrow"
         android:contentDescription="@string/desc_prev" android:focusable="true" />
      <ImageButton
         android:id="@+id/nextmonth"
         android:layout_alignParentRight="true" android:paddingLeft="25dp"
         android:src="@drawable/selector_next" style="@style/Img_Arrow"
         android:contentDescription="@string/desc_next" android:focusable="true" />
   </RelativeLayout>
   <LinearLayout
      android:id="@+id/weekdaysll"
      android:orientation="horizontal"
      android:layout_width="match_parent" android:layout_height="0dp"
      android:layout_weight="6">
      <TextView android:id="@+id/day1" style="@style/Text_DaysOfWeek"/>
      <TextView android:id="@+id/day2" style="@style/Text_DaysOfWeek"/>
      <TextView android:id="@+id/day3" style="@style/Text_DaysOfWeek"/>
      <TextView android:id="@+id/day4" style="@style/Text_DaysOfWeek"/>
      <TextView android:id="@+id/day5" style="@style/Text_DaysOfWeek"/>
      <TextView android:id="@+id/day6" style="@style/Text_DaysOfWeek"/>
      <TextView android:id="@+id/day7" style="@style/Text_DaysOfWeek"/>
   </LinearLayout>
   <LinearLayout
      android:id="@+id/week1ll"
      android:orientation="horizontal" style="@style/LL_Week">
      <TextView android:id="@+id/date11" style="@style/Text_Dates"/>
      <TextView android:id="@+id/date12" style="@style/Text_Dates"/>
      <TextView android:id="@+id/date13" style="@style/Text_Dates"/>
      <TextView android:id="@+id/date14" style="@style/Text_Dates"/>
      <TextView android:id="@+id/date15" style="@style/Text_Dates"/>
      <TextView android:id="@+id/date16" style="@style/Text_Dates"/>
      <TextView android:id="@+id/date17" style="@style/Text_Dates"/>
   </LinearLayout>
   <LinearLayout
      android:id="@+id/week2ll"
      android:orientation="horizontal" style="@style/LL_Week">
      <TextView android:id="@+id/date21" style="@style/Text_Dates"/>
